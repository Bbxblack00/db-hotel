FROM `ospiti`


2 Quesito

SELECT *
FROM `ospiti`
WHERE `document type` LIKE 'CI'


3 Quesito

SELECT *
FROM `paganti`
WHERE ospite_id != null


4 Quesito

SELECT *
FROM `stanze`
WHERE room_number = 1


5 Quesito

