1 Quesito

SELECT *
FROM `ospiti`


2 Quesito

SELECT *
FROM `ospiti`
WHERE `document type` LIKE 'CI'


3 Quesito

SELECT *
FROM `paganti`
WHERE ospite_id != null


4 Quesito

SELECT *
FROM `stanze`
WHERE room_number = 1


5 Quesito

SELECT *
FROM `ospiti`
WHERE LOCATE ('E', name) LIKE 1

SELECT *
FROM `ospiti`
WHERE name LIKE 'E%'


7 Quesito

SELECT *
FROM `ospiti`
WHERE (YEAR(NOW()) - YEAR(date_of_birth)) < 30


8 Quesito

SELECT *
FROM prenotazioni
WHERE YEAR(created_at) <= 2018
    AND MONTH(created_at) < 5

SELECT *
FROM `prenotazioni`
WHERE created_at < "2018-05"
